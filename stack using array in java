class stack{
    static final int MAX=100;
    int top=-1;
    int a[]=new int [MAX];
    
    void  push(int data){
        a[++top]=data;
    }
    void pop(){
        top--;
    }
    void display(){
        for(int i=top;i>-1;i--)System.out.println(a[i]);
        
    }
    public static void main(String args[]){
        stack s=new stack();
        s.push(10);
        s.push(20);
        s.push(30);
        s.display();
    }
    
}
