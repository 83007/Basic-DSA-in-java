class HelloWorld {
static void insertionsort(int arr[],int n){
    for(int i=1;i<n;i++){
    int j=i-1,key=arr[i];
    while(j>=0&& arr[j]>key)
{        
    arr[j+1]=arr[j];
    j=j-1;
}
arr[j+1]=key;
    }
}
static void print(int arr[],int n){
    for(int i=0;i<n;i++)System.out.print(arr[i]+" ");
}
    public static void main(String[] args) {
int arr[]={3,5,1,7,2
    };
    int n=arr.length;
    insertionsort( arr, n);
    print( arr, n);
    
}
    
}
